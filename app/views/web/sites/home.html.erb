<div class="container my-5">
  <table class="table table-striped table-dark table-hover">
    <thead class="text-white">
      <tr>
        <th>Name</th>
        <th>Symbol</th>
        <th>Price</th>
        <th>Dividend</th>
      </tr>
    </thead>
    <tbody class="text-white">
    <% @stocks.each do | stock | %>
      <tr>
        <th style="color:#239C8F"><%= stock.name %></th>
        <th><%= stock.symbol%></th>
        <th><%= stock.price %></th>
        <th>Yes</th>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
